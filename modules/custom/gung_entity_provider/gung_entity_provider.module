<?php

/**
 * @file
 * Contains gung_entity_provider.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function gung_entity_provider_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the gung_entity_provider module.
    case 'help.page.gung_entity_provider':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Gung&#039;s demo module generated by Drupal Console') . '</p>';
      return $output;

    default:
  }
}

/**
 * Implements hook_theme().
 */
function gung_entity_provider_theme() {
  return [
    'gung_entity_provider' => [
      'render element' => 'children',
    ],
  ];
}
